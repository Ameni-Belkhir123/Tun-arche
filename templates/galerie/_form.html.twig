{{ form_start(form, {'attr': {'class': 'form', 'enctype': 'multipart/form-data'}}) }}
<div class="mb-3">
    {{ form_label(form.categorie) }}
    {{ form_widget(form.categorie, {'attr': {'class': 'form-control', 'placeholder': 'Enter category'}}) }}
</div>
<div class="mb-3">
    {{ form_label(form.nom) }}
    {{ form_widget(form.nom, {'attr': {'class': 'form-control', 'placeholder': 'Enter name'}}) }}
</div>
<div class="mb-3">
    {{ form_label(form.image) }}
    {{ form_widget(form.image, {'attr': {'class': 'form-control'}}) }}
</div>
<button class="btn btn-success">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
