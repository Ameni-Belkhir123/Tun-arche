<!doctype html>
<html lang="en">
  <head>
    <title>Login | Dashboardkit Dashboard Template</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=0,minimal-ui">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Dashboardkit is trending dashboard template made using Bootstrap 5 design framework. Dashboardkit is available in Bootstrap, React, CodeIgniter, Angular, and .net Technologies.">
    <meta name="keywords" content="Bootstrap admin template, Dashboard UI Kit, Dashboard Template, Backend Panel, react dashboard, angular dashboard">
    <meta name="author" content="Codedthemes">
    <link rel="icon" href="{{ asset('back1/temp/dashboardkit.io/assets/images/favicon.svg') }}" type="image/x-icon">
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@700..700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="{{ asset('back1/temp/dashboardkit.io/assets/fonts/phosphor/duotone/style.css') }}">
    <link rel="stylesheet" href="{{ asset('back1/temp/dashboardkit.io/assets/fonts/tabler-icons.min.css') }}">
    <link rel="stylesheet" href="{{ asset('back1/temp/dashboardkit.io/assets/fonts/feather.css') }}">
    <link rel="stylesheet" href="{{ asset('back1/temp/dashboardkit.io/assets/fonts/fontawesome.css') }}">
    <link rel="stylesheet" href="{{ asset('back1/temp/dashboardkit.io/assets/fonts/material.css') }}">
    <link rel="stylesheet" href="{{ asset('back1/temp/dashboardkit.io/assets/css/style.css') }}" id="main-style-link">
    <link rel="stylesheet" href="{{ asset('back1/temp/dashboardkit.io/assets/css/style-preset.css') }}">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
  </head>
  <body data-pc-preset="preset-1" data-pc-sidebar-theme="dark" data-pc-header-theme="light" data-pc-sidebar-caption="true" data-pc-direction="ltr" data-pc-theme="light">
    <div class="loader-bg">
      <div class="pc-loader">
        <div class="loader-fill"></div>
      </div>
    </div>
    <div class="auth-main v2">
      <div class="bg-overlay bg-dark"></div>
      <div class="auth-wrapper">
        <div class="auth-sidecontent">
          <div class="text-start px-3 px-md-5 pb-3 pb-md-5">
            <img src="{{ asset('back1/temp/dashboardkit.io/assets/images/logo-white.svg') }}" alt="" class="img-fluid">
            <h1 class="text-white my-4">Welcome you!</h1>
            <h4 class="text-white font-weight-normal">Signup to your account and made member of the Dashboard-kit admin Template.</h4>
            <h4 class="text-white font-weight-normal">Do not forget to play with live customizer</h4>
          </div>
        </div>
        <div class="auth-form">
          <div class="card mb-0">
            <div class="card-body">
              <div class="auth-content">
                <img src="{{ asset('back1/temp/dashboardkit.io//assets/images/logo-dark.svg') }}" alt="" class="img-fluid mb-4 d-block d-xl-none d-lg-none">
                <h4 class="mb-3 f-w-400">Login</h4>
                {% if error and error.messageKey != 'Not privileged to request the resource.' %}
                  <div class="alert alert-danger">
                    {{ error.messageKey|trans(error.messageData, 'security') }}
                  </div>
                {% endif %}
                <form action="{{ path('login') }}" method="POST" id="login-form">
                  <div class="input-group mb-3">
                    <span class="input-group-text">
                      <i data-feather="user"></i>
                    </span>
                    <input type="text" id="email" name="_email" class="form-control" placeholder="Email">
                  </div>
                  <div class="input-group mb-4">
                    <span class="input-group-text">
                      <i data-feather="lock"></i>
                    </span>
                    <input type="password" id="password" name="_password" class="form-control" placeholder="Password">
                  </div>
                  <div class="mb-3 mt-2">
                    <div class="form-check text-start">
                      <input class="form-check-input" type="checkbox" value="" id="flexCheckChecked" checked>
                      <label class="form-check-label" for="flexCheckChecked">
                        Send me the <a href="#!">Newsletter</a> weekly.
                      </label>
                    </div>
                  </div>
                  <input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}">
                  <div class="g-recaptcha" data-sitekey="{{ recaptcha_site_key }}"></div>
                  <button class="btn btn-primary btn-block mb-4">Login</button>
                </form>
              </div>
              <div class="saprator my-3">
                <span>OR</span>
              </div>
              <div class="row g-2">
                <div class="col-4">
                  <div class="d-grid">
                    <button type="button" class="btn mt-2 btn-light-primary bg-light text-muted">
                      <img src="{{ asset('back1/temp/dashboardkit.io/assets/images/authentication/google.svg') }}" alt="img">
                      <span class="d-none d-sm-inline-block">Google</span>
                    </button>
                  </div>
                </div>
                <div class="col-4">
                  <div class="d-grid">
                    <button type="button" class="btn mt-2 btn-light-primary bg-light text-muted">
                      <img src="{{ asset('back1/temp/dashboardkit.io/assets/images/authentication/twitter.svg') }}" alt="img">
                      <span class="d-none d-sm-inline-block">Twitter</span>
                    </button>
                  </div>
                </div>
                <div class="col-4">
                  <div class="d-grid">
                    <button type="button" class="btn mt-2 btn-light-primary bg-light text-muted">
                      <img src="{{ asset('back1/temp/dashboardkit.io/assets/images/authentication/facebook.svg') }}" alt="img">
                      <span class="d-none d-sm-inline-block">Facebook</span>
                    </button>
                  </div>
                </div>
              </div>
              <p class="mt-4">
                Donâ€™t have an account?
                <a href="{{ path('home01') }}" class="f-w-400">Register</a>
              </p>
              <p>
                Forgot your password?
                <a href="{{ path('forgot_password') }}" class="f-w-400">Reset it here</a>
              </p>
            </div>
          </div>
        </div>
      </div>
    </div>
    {% for flash in app.flashes('registration_success') %}
      <script>
        document.addEventListener("DOMContentLoaded", function() {
          $('#registrationModal').modal('show');
        });
      </script>
    {% endfor %}
    <div class="modal fade" id="registrationModal" tabindex="-1" role="dialog" aria-labelledby="registrationModalLabel" aria-hidden="true">
      <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="registrationModalLabel">Registration Successful</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
            Your registration was successful. Please check your email and click the verification link to activate your account.
          </div>
          <div class="modal-footer">
            <a href="{{ path('login') }}" class="btn btn-primary">OK</a>
          </div>
        </div>
      </div>
    </div>
    <script src="{{ asset('back1/temp/dashboardkit.io/assets/js/plugins/popper.min.js') }}"></script>
    <script src="{{ asset('back1/temp/dashboardkit.io/assets/js/plugins/simplebar.min.js') }}"></script>
    <script src="{{ asset('back1/temp/dashboardkit.io/assets/js/plugins/bootstrap.min.js') }}"></script>
    <script src="{{ asset('back1/temp/dashboardkit.io/assets/js/plugins/i18next.min.js') }}"></script>
    <script src="{{ asset('back1/temp/dashboardkit.io/assets/js/plugins/i18nextHttpBackend.min.js') }}"></script>
    <script src="{{ asset('back1/temp/dashboardkit.io/assets/js/fonts/custom-font.js') }}"></script>
    <script src="{{ asset('back1/temp/dashboardkit.io/assets/js/pcoded.js') }}"></script>
    <script src="{{ asset('back1/temp/dashboardkit.io/assets/js/theme.js') }}"></script>
    <script src="{{ asset('back1/temp/dashboardkit.io/assets/js/multi-lang.js') }}"></script>
    <script src="{{ asset('back1/temp/dashboardkit.io/assets/js/plugins/feather.min.js') }}"></script>
    <script src="https://www.google.com/recaptcha/api.js" async defer></script>
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
  </body>
</html>
